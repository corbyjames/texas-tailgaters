{
  "name": "texas-tailgaters",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "postbuild": "npm run test:regression",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "playwright test",
    "test:regression": "playwright test tests/regression",
    "test:mobile": "playwright test --project=mobile-chrome --project=mobile-safari",
    "test:desktop": "playwright test --project=desktop-chrome --project=desktop-firefox",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:report": "playwright show-report",
    "test:e2e": "playwright test -c e2e/playwright.config.ts",
    "test:e2e:headed": "playwright test -c e2e/playwright.config.ts --headed",
    "test:e2e:debug": "playwright test -c e2e/playwright.config.ts --debug",
    "test:smoke": "playwright test -c e2e/playwright.config.ts --grep \"smoke\"",
    "test:auth": "playwright test -c e2e/playwright.config.ts e2e/auth",
    "test:games": "playwright test -c e2e/playwright.config.ts e2e/games",
    "test:potluck": "playwright test -c e2e/playwright.config.ts e2e/potluck",
    "test:admin": "playwright test -c e2e/playwright.config.ts e2e/admin",
    "test:visual": "playwright test -c e2e/playwright.config.ts e2e/visual",
    "test:mobile": "playwright test -c e2e/playwright.config.ts --project=mobile-chrome",
    "test:update-snapshots": "playwright test -c e2e/playwright.config.ts --update-snapshots",
    "test:report": "playwright show-report",
    "test:install": "playwright install",
    "test:production": "playwright test --config=playwright.production.config.ts",
    "test:production:ui": "playwright test --config=playwright.production.config.ts --ui",
    "test:production:quick": "node test-production.js"
  },
  "dependencies": {
    "@emailjs/browser": "^4.3.3",
    "@supabase/supabase-js": "^2.56.0",
    "firebase": "^12.2.1",
    "lucide-react": "^0.350.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.22.0",
    "socket.io-client": "^4.6.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.17",
    "playwright": "^1.55.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "vite": "^5.4.0"
  }
}
